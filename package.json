{
  "name": "metis-ui",
  "version": "1.1.1",
  "description": "MetisUI - The React Tailwind UI Component Library",
  "license": "MIT",
  "authors": [
    "wangjianming.x@gmail.com"
  ],
  "homepage": "https://metisui.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/metisjs/metis-ui.git"
  },
  "keywords": [
    "metisui",
    "metis-ui",
    "metis ui",
    "tailwind",
    "tailwindcss",
    "tailwind css",
    "tailwind react",
    "tailwind plugin",
    "tailwind component",
    "tailwind css plugin",
    "antd",
    "ant design",
    "Ant Design",
    "react",
    "css",
    "ui",
    "component",
    "framework",
    "front-end",
    "theme",
    "component library",
    "css library"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib",
    "locale",
    "plugin",
    "theme"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install && dumi setup",
    "release": "node ./scripts/release",
    "start": "npm run dev",
    "sort:api-table": "node ./scripts/sort-api-table"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@tailwindcss/cli": "^4.0.17",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^20.0.0",
    "@types/qs": "^6.9.15",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@types/react-highlight-words": "^0.20.0",
    "@types/react-resizable": "^3.0.8",
    "@types/throttle-debounce": "^5.0.2",
    "@umijs/lint": "4.4.6",
    "@umijs/utils": "4.4.6",
    "babel-plugin-import": "^1.13.8",
    "dumi": "~2.4.21",
    "eslint": "^8.39.0",
    "father": "~4.5.2",
    "fetch-jsonp": "^1.3.0",
    "fs-extra": "^11.3.0",
    "git-repo-info": "^2.1.1",
    "glob": "^11.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "majo": "^0.9.1",
    "postcss-nested": "^7.0.2",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "qs": "^6.11.2",
    "react": "^19.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.0",
    "react-draggable": "^4.4.6",
    "react-fast-marquee": "^1.6.4",
    "react-highlight-words": "^0.20.0",
    "react-intersection-observer": "^9.16.0",
    "react-resizable": "^3.0.5",
    "remark-frontmatter": "^2.0.0",
    "remark-parse": "^8.0.0",
    "remark-stringify": "^8.0.0",
    "remark-yaml-config": "^4.1.0",
    "rimraf": "^5.0.7",
    "tailwindcss": "^4.0.17",
    "typescript": "~5.0.4",
    "umi": "^4.4.6",
    "unified": "^8.0.0",
    "zx": "^8.1.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=4"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.2",
    "@metisjs/icons": "^1.1.0",
    "@rc-component/async-validator": "^5.0.4",
    "@rc-component/context": "^1.4.0",
    "@rc-component/mini-decimal": "^1.1.0",
    "@rc-component/mutate-observer": "^1.1.0",
    "@rc-component/portal": "^1.1.2",
    "@rc-component/util": "^1.2.1",
    "@types/lodash": "^4.14.194",
    "ahooks": "^3.8.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.12",
    "lodash": "^4.17.21",
    "query-string": "^9.1.1",
    "rc-field-form": "2.4.0",
    "rc-overflow": "1.3.1",
    "rc-progress": "^4.0.0",
    "rc-resize-observer": "^1.4.0",
    "react-virtuoso": "^4.12.0",
    "scroll-into-view-if-needed": "^3.0.10",
    "tailwind-merge": "^3.0.2",
    "throttle-debounce": "^5.0.0",
    "tyme4ts": "^1.1.6"
  },
  "pnpm": {
    "patchedDependencies": {
      "rc-overflow@1.3.1": "patches/rc-overflow@1.3.1.patch",
      "@rc-component/context@1.4.0": "patches/@rc-component__context@1.4.0.patch"
    }
  }
}
