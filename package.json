{
  "name": "metis-ui",
  "version": "0.0.1-alpha.1",
  "description": "A react library developed with dumi",
  "license": "MIT",
  "main": "lib/components/index.js",
  "module": "es/components/index.js",
  "typings": "lib/components/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install && dumi setup",
    "release": "node ./scripts/release",
    "start": "npm run dev",
    "sort:api-table": "node ./scripts/sort-api-table"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.3.1",
    "@types/node": "^20.0.0",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/react-highlight-words": "^0.20.0",
    "@types/react-resizable": "^3.0.8",
    "@types/throttle-debounce": "^5.0.2",
    "@umijs/lint": "4.3.1",
    "@umijs/plugins": "4.3.1",
    "@umijs/utils": "4.3.1",
    "babel-plugin-import": "^1.13.8",
    "dumi": "~2.4.13",
    "eslint": "^8.39.0",
    "father": "^4.5.1",
    "fetch-jsonp": "^1.3.0",
    "git-repo-info": "^2.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "majo": "^0.9.1",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.4.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "qs": "^6.11.2",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-draggable": "^4.4.6",
    "react-fast-marquee": "^1.6.4",
    "react-highlight-words": "^0.20.0",
    "react-resizable": "^3.0.5",
    "react-sticky-box": "^2.0.5",
    "remark-frontmatter": "^2.0.0",
    "remark-parse": "^8.0.0",
    "remark-stringify": "^8.0.0",
    "remark-yaml-config": "^4.1.0",
    "rimraf": "^5.0.7",
    "stylelint": "^15.6.0",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.0.4",
    "unified": "^8.0.0",
    "zx": "^8.1.2"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0",
    "tailwindcss": ">=3"
  },
  "publishConfig": {
    "access": "public"
  },
  "authors": [
    "wangjianming.x@gmail.com"
  ],
  "dependencies": {
    "@ant-design/fast-color": "^2.0.6",
    "@ctrl/tinycolor": "^4.1.0",
    "@metisjs/icons": "^0.0.2-alpha.5",
    "@rc-component/async-validator": "^5.0.4",
    "@rc-component/context": "^1.4.0",
    "@rc-component/mini-decimal": "^1.1.0",
    "@rc-component/mutate-observer": "^1.1.0",
    "@rc-component/portal": "^1.1.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.194",
    "ahooks": "^3.8.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.12",
    "lodash": "^4.17.21",
    "rc-field-form": "2.4.0",
    "rc-overflow": "1.3.1",
    "rc-progress": "^4.0.0",
    "rc-resize-observer": "^1.4.0",
    "rc-util": "^5.43.0",
    "react-virtuoso": "^4.12.0",
    "scroll-into-view-if-needed": "^3.0.10",
    "tailwind-merge": "^2.3.0",
    "throttle-debounce": "^5.0.0",
    "tyme4ts": "^1.1.6"
  },
  "pnpm": {
    "patchedDependencies": {
      "rc-overflow@1.3.1": "patches/rc-overflow@1.3.1.patch",
      "@umijs/plugins@4.3.1": "patches/@umijs__plugins@4.3.1.patch",
      "@rc-component/context@1.4.0": "patches/@rc-component__context@1.4.0.patch"
    }
  }
}
